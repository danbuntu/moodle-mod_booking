{{=<% %>=}}

<%#arguments%>
            <% key %>
            <br>
            <% value %>
<%/arguments%>

<div>
<%#mybookings%>
    <ion-card>
        <ion-card-header>
            <% text %>
        </ion-card-header>
        <ion-card-content>
            <b>
            {{ 'plugin.mod_booking.mybookingsbooking' | translate }}
            </b> <% name %> (<% fullname %>) <br>
            <b>
            {{ 'plugin.mod_booking.description' | translate }}
            </b> <% description %> <br>

            <ion-button core-site-plugins-new-content
                title="<% certificate.name %>" component="mod_booking"
                method="mobile_booking_option_details" [args]="{optionid: <% optionid %>, userid: <% userid %>}">
            {{ 'plugin.mod_booking.details' | translate }}
        </ion-button>
        </ion-card-content>
    </ion-card>
<%/mybookings%>
</div>